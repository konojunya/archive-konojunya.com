{
  "name": "konojunya.com",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack --progress --color",
    "build:watch": "webpack --progress --color -w",
    "server": "pm2 start pm2/config.json --no-daemon",
    "dev": "concurrently \"npm run build:watch\" \"npm run server\" \"npm run test:watch\"",
    "test": "ava --verbose",
    "test:watch": "ava --verbose -w",
    "cover": "nyc --reporter=lcov --reporter=text npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://konojunya@github.com/konojunya/konojunya.com.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/konojunya/konojunya.com/issues"
  },
  "homepage": "https://github.com/konojunya/konojunya.com#readme",
  "dependencies": {
    "express": "^4.15.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-ga": "^2.2.0",
    "react-tap-event-plugin": "^2.0.1"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "jsdom": "^8.3.1",
    "node-sass": "^4.5.3",
    "nyc": "^11.0.3",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.1.0"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "powerAssert": true,
    "babel": "inherit"
  }
}
